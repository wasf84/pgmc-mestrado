@article{ WOS:000414160500074,
Author = {Ni, Zhuobiao and van Gaans, Pauline and Rijnaarts, Huub and Grotenhuis,
   Tim},
Title = {Combination of aquifer thermal energy storage and enhanced
   bioremediation: Biological and chemical clogging},
Journal = {SCIENCE OF THE TOTAL ENVIRONMENT},
