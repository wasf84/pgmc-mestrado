@inproceedings{ WOS:000454932300031,
Author = {Nasution, T. H. and Siagian, E. C. and Tanjung, K. and Soeharwinto},
Book-Group-Author = {IOP},
Title = {Design of river height and speed monitoring system by using Arduino},
Booktitle = {10TH INTERNATIONAL CONFERENCE NUMERICAL ANALYSIS IN ENGINEERING},
Series = {IOP Conference Series-Materials Science and Engineering},
Year = {2018},
Volume = {308},
Note = {10th International Conference Numerical Analysis in Engineering (NAE),
   BANDA ACEH, INDONESIA, AUG 24-25, 2017},
Abstract = {River is one part of the hydrologic cycle. Water in rivers is generally
   collected from precipitation, such as rain, dew, springs, underground
   runoff, and in certain countries also comes from melt ice/snow. The
   height and speed of water in a river is always changing. Changes in
   altitude and speed of water can affect the surrounding environment. In
   this paper, we will design a system to measure the altitude and speed of
   the river. In this work we use Arduino Uno, ultrasonic sensors and flow
